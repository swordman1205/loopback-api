{
  "db": {
    "host": "localhost",
    "port": 27017,
    "database": "cooksystem",
    "name": "db",
    "connector": "mongodb",
    "server": {
      "auto_reconnect": true,
      "reconnectTries": 100,
      "reconnectInterval": 1000
    }
  },
  "files": {
    "name": "files",
    "connector": "loopback-component-storage",
    "provider": "filesystem",
    "root": "./storage"
  }
}
